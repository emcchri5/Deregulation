MN3 <- revenue.annual.ind.share %>%
  filter(state == "MN") %>%
  filter(year >= 1990) %>%
  ggplot(mapping = aes(x = year, y = percent, color = service)) +
  geom_line() +
  scale_x_continuous(limit=c(1990, 2018), breaks = (c(1990, 1995, 2000, 2005, 2010, 2015)), minor_breaks = (seq(1990, 2018, 1))) +
  scale_y_continuous(limit = c(0,100), breaks = c(0,25,50,75)) +
  xlab("Year") +
  ylab("Percent of Total Revenue (%)") +
  labs(color = "Industry Sector Category") +
  ggtitle("Minnesota Share of Market Revenue by Sector") +

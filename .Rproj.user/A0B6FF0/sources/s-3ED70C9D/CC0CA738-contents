#Loading EIA Data into Environment
```{r}
gen.annual.origin <- read_csv("/Users/EthanMcChristian/Downloads/annual_generation_state.csv")
```
#Wrangling generation data (gen.annual comes back later)
```{r}
gen.annual <- gen.annual.origin %>%
  rename(year = YEAR) %>%
  rename(state = STATE) %>%
  rename(producertype = "TYPE OF PRODUCER") %>%
  rename(source = "ENERGY SOURCE") %>%
  rename(gen = "GENERATION (Megawatthours)")
gen.annual
```
#Making the data set wide by source, so that I have "gen" (generation in kilowatt hours) in terms of the source it comes from
```{r}
gen.annual.prod.origin <- gen.annual.origin %>%
  rename(year = YEAR, state = STATE, prod = "TYPE OF PRODUCER", source = "ENERGY SOURCE", gen = "GENERATION (Megawatthours)") %>%
  dplyr::select(year, state, prod, source, gen) %>%
  spread(source, gen)
gen.annual.prod.origin
```
#just creating a negation in shortcut
```{r}
"%nin%" = Negate('%in%') 
```
#looking at US-totals, but they named the columns two different things
```{r}
gen.annual.prod <- gen.annual.prod.origin %>%
  filter(state %in% c("US-TOTAL", "US-Total")) %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`)
gen.annual.prod
```
#Summing renewables (total.ren), share of renewables out of total (share.ren), and total.nonren
```{r}
gen.annual.prod.total <- gen.annual.prod %>%
  group_by(year, prod) %>%
  mutate(totalren = sum(geothermal, wind, solar, hydro, nuclear, na.rm = TRUE)) %>%
  mutate(share.ren = totalren/total) %>%
  dplyr::select(year, total, totalren, share.ren, prod) %>%
  distinct() %>%
  mutate(total.nonren = total - totalren)
gen.annual.prod.total
```
#plotting the data to look at trends in share.ren, totalren, total.nonren, and total by producer type ()
```{r}
ggplot(data = gen.annual.prod.total, mapping = aes(x = year, y = share.ren, color = prod)) +
  geom_line()
ggplot(data = gen.annual.prod.total, mapping = aes(x=year, y = totalren, color = prod)) +
  geom_line()
ggplot(data = gen.annual.prod.total, mapping = aes(x=year, y = total.nonren, color = prod)) +
  geom_line()
ggplot(data = gen.annual.prod.total, mapping = aes(x=year, y = total, color = prod)) +
  geom_line()
```
#generation is in megawatthours, sorted by electric generators (utilities and independent power producers)
```{r}
gen.annual.prelong <- gen.annual %>%
  dplyr::select(year, state, producertype, source, gen) %>%
  filter(producertype %in% c("Electric Generators, Electric Utilities", "Electric Generators, Independent Power Producers")) %>%
  spread(producertype, gen)
gen.annual
gen.annual.prelong
```
#creating columns for source, splitting the database up into two with respect to utilities and independent power producers, stitching databases back together
```{r}
gen.annual.wide.ut <- gen.annual.prelong %>%
  dplyr::select(year, state, source, "Electric Generators, Electric Utilities") %>%
  spread(source, "Electric Generators, Electric Utilities") %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`) %>%
   group_by(year, state) %>%
  mutate(total.ren = sum(geothermal, hydro, pumped, solar, wind, wood, otherbio, na.rm= TRUE)) %>%
  mutate(ind = "Full-Service Providers")
gen.annual.wide.ut
gen.annual.wide.ipp <- gen.annual.prelong %>%
  dplyr::select(year, state, source, "Electric Generators, Independent Power Producers") %>%
  spread(source, "Electric Generators, Independent Power Producers") %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`) %>%
  group_by(year, state) %>%
  mutate(total.ren = sum(geothermal, hydro, pumped, solar, wind, wood, otherbio, na.rm= TRUE)) %>%
  mutate(ind = "Restructured Retail Service Providers")
gen.annual.wide.ipp
gen.annual.wide.ut
gen.annual.wide.ipput <- full_join(gen.annual.wide.ut, gen.annual.wide.ipp) %>%
  filter(year >=1990)
gen.annual.wide.ipput 
```
#Keying dataset by year and state, and adding back in totalren, totalnonren, and renpercent
```{r}
gen.annual.long.total <- gen.annual %>%
  dplyr::select(year, state, producertype, source, gen) %>%
  spread(source, gen) %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`) %>%
  filter(producertype == "Total Electric Power Industry") %>%
  group_by(year, state, producertype) %>%
  mutate(totalren = sum(geothermal, hydro, solar, wind, pumped, other, wood, otherbio, na.rm = TRUE)) %>%
  mutate(totalnonren = sum(coal, natgas, nuclear, othergas, pet, na.rm = TRUE)) %>%
  mutate(renpercent = totalren/total)

```
#for graphing
```{r}
gen.annual.long <- gen.annual %>%
  dplyr::select(year, state, producertype, source, gen) %>%
  spread(source, gen) %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`) %>%
  group_by(year, state, producertype) %>%
  mutate(totalren = sum(geothermal, hydro, solar, wind, pumped, other, wood, otherbio, na.rm = TRUE)) %>%
  mutate(totalnonren = sum(coal, natgas, nuclear, othergas, pet, na.rm = TRUE)) %>%
  mutate(renpercent = totalren/total)
gen.annual.long.2 <- gen.annual %>%
  dplyr::select(year, state, producertype, source, gen) %>%
  mutate(gen = gen/1000000) %>%
  spread(source, gen) %>%
  rename(coal = Coal, geothermal = Geothermal, hydro = `Hydroelectric Conventional`, natgas = `Natural Gas`, nuclear = Nuclear, other = Other, otherbio = `Other Biomass`, othergas = `Other Gases`, pet = Petroleum, pumped = `Pumped Storage`, solar = `Solar Thermal and Photovoltaic`, total = Total, wind = Wind, wood = `Wood and Wood Derived Fuels`) %>%
  group_by(year, state, producertype) %>%
  mutate(totalren = sum(geothermal, hydro, solar, wind, pumped, other, wood, otherbio, na.rm = TRUE)) %>%
  mutate(totalnonren = sum(coal, natgas, nuclear, othergas, pet, na.rm = TRUE)) %>%
  mutate(renpercent = totalren/total)

```

#Price Data, in cents/kilowatthour
```{r}
price.annual.origin1 <- read_csv("/Users/EthanMcChristian/Downloads/avgprice.csv")
price.annual.origin <- price.annual.origin1 %>% 
  rename(ind = "Industry Sector Category") %>%
  filter(ind %in% c("Total Electric Industry", "Restructured Retail Service Providers", "Full-Service Providers")) %>%
  rename("Industry Sector Category" = ind)
price.annual.origin
```
```{r}
price.annual1 <- price.annual.origin %>%
  rename(year = Year, state = State, ind = "Industry Sector Category", res.price = Residential, comm.price = Commercial, ind.price = Industrial, trans.price = Transportation, other.price = Other, total.price = Total) %>%
  filter(ind %in% c("Restructured Retail Service Providers", "Full-Service Providers"))
price.annual1
```

#filter by Total Electric Industry (to match gen.annual)
```{r}
price.annual.total <- price.annual1 %>%
  filter(industry == "Total Electric Industry") %>%
  dplyr::select(year, state, res.price, comm.price, ind.price, trans.price, other.price, total.price)
price.annual.total
```
#Importing revenue values in thousands of dollars
```{r}
revenue.annual.origin1 <- read_csv("/Users/EthanMcChristian/Downloads/revenue_annual.csv")
revenue.annual.origin <- revenue.annual.origin1 %>% 
  rename(ind = "Industry Sector Category") %>%
  filter(ind %in% c("Total Electric Industry", "Restructured Retail Service Providers", "Full-Service Providers")) %>%
  rename("Industry Sector Category" = ind)
revenue.annual.origin
```

```{r}
revenue.annual1 <- revenue.annual.origin %>%
  rename(year = Year, state = State, ind = "Industry Sector Category", res.rev = Residential, comm.rev = Commercial, ind.rev = Industrial, trans.rev = Transportation, other.rev = Other, total.rev = Total) %>%
  filter(ind %in% c("Restructured Retail Service Providers", "Full-Service Providers"))
revenue.annual1
```
#wrangling annual revenue, filtering by total electric industry (I should revisit this, it's not looking at everythign I want)
```{r}
revenue.annual.total <- revenue.annual.origin %>%
  rename(year = Year, state = State, ind = "Industry Sector Category", res.rev = Residential, comm.rev = Commercial, ind.rev = Industrial, trans.rev = Transportation, other.rev = Other, total.rev = Total) %>%
  dplyr::select(year, state, res.rev, comm.rev, ind.rev, trans.rev, other.rev, total.rev, ind) %>%
  filter(ind == "Total Electric Industry")
revenue.annual.total
```
#creating a *wide* database that shows revenue per industry sector
```{r}
revenue.annual.ind <- revenue.annual.origin %>%
  rename(year = Year, state = State, ind = "Industry Sector Category", res.rev = Residential, comm.rev = Commercial, ind.rev = Industrial, trans.rev = Transportation, other.rev = Other, total.rev = Total) %>%
  dplyr::select(year,state,total.rev,ind)
revenue.annual.ind <- revenue.annual.ind %>%
  spread(ind, total.rev)
revenue.annual.ind %>% 
  group_by(year, state) %>%
  rename(DOS = "Delivery-Only Service", EOP = "Energy-Only Providers", RRSP = "Restructured Retail Service Providers", FSP = "Full-Service Providers", TEI = "Total Electric Industry") %>%
  mutate(test = RRSP + FSP, na.rm = TRUE) %>%
  filter(year>=1990) %>%
  dplyr::select(test, TEI)
```
#manipulating industry sector revenues to show % of total per industry sector
```{r}
revenue.annual.ind.share <- revenue.annual.ind %>%
  rename(deliveryonly = "Delivery-Only Service", energyonly = "Energy-Only Providers", fullservice = "Full-Service Providers", retail = "Restructured Retail Service Providers", total = "Total Electric Industry") %>%
  mutate(share.full = fullservice/total) %>%
  mutate(share.retail = retail/total) %>%
  dplyr::select(year, state, share.full, share.retail) %>%
  rename("Full Service Providers" = share.full, "Restructured Retail Service Providers" = share.retail) %>%
  gather(service, share, c("Full Service Providers","Restructured Retail Service Providers")) %>%
  mutate(percent = share*100)
revenue.annual.ind.share
```
#Combining datasets for total
```{r}
combo1 <- left_join(
  price.annual.total,
  revenue.annual.total
)
trio_data.total <- left_join(
  combo1,
  gen.annual.long.total
)
combo2 <- left_join(
  price.annual1,
  revenue.annual1
)
trio_data.ind <- left_join(
  combo2,
  gen.annual.wide.ipput
)
trio_data.ind
trio_data.total
```
#adding deregulation dummies to states
```{r}
WA <- trio_data.total %>% 
  filter(state == "WA") %>%
  group_by(year) %>%
  mutate(dereg = 0) %>%
  mutate(part.dereg = 0)
OR <- trio_data.total %>% 
  filter(state == "OR") %>%
  group_by(year) %>%
  mutate(full.dereg = 0) %>%
  mutate(part.dereg = if(year >= 2001) {1} else {0})

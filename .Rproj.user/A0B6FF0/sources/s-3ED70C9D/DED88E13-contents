#creating columns for source, splitting the database up into two with respect to utilities and independent power producers, stitching databases back together

```r
gen.annual.wide.ut <- gen.annual.prelong %>%
  dplyr::select(year, state, source, "Electric Generators, Electric Utilities") %>%
  spread(source, "Electric Generators, Electric Utilities") %>%
  rename(coal.ut = Coal, geothermal.ut = Geothermal, hydro.ut = `Hydroelectric Conventional`, natgas.ut = `Natural Gas`, nuclear.ut = Nuclear, other.ut = Other, otherbio.ut = `Other Biomass`, othergas.ut = `Other Gases`, pet.ut = Petroleum, pumped.ut = `Pumped Storage`, solar.ut = `Solar Thermal and Photovoltaic`, total.ut = Total, wind.ut = Wind, wood.ut = `Wood and Wood Derived Fuels`) %>%
   group_by(year, state) %>%
  mutate(total.ren.ut = sum(geothermal.ut, hydro.ut, pumped.ut, solar.ut, wind.ut, wood.ut, otherbio.ut, na.rm= TRUE))
gen.annual.wide.ipp <- gen.annual.prelong %>%
  dplyr::select(year, state, source, "Electric Generators, Independent Power Producers") %>%
  spread(source, "Electric Generators, Independent Power Producers") %>%
  rename(coal.ipp = Coal, geothermal.ipp = Geothermal, hydro.ipp = `Hydroelectric Conventional`, natgas.ipp = `Natural Gas`, nuclear.ipp = Nuclear, other.ipp = Other, otherbio.ipp = `Other Biomass`, othergas.ipp = `Other Gases`, pet.ipp = Petroleum, pumped.ipp = `Pumped Storage`, solar.ipp = `Solar Thermal and Photovoltaic`, total.ipp = Total, wind.ipp = Wind, wood.ipp = `Wood and Wood Derived Fuels`) %>%
  group_by(year, state) %>%
  mutate(total.ren.ipp = sum(geothermal.ipp, hydro.ipp, pumped.ipp, solar.ipp, wind.ipp, wood.ipp, otherbio.ipp, na.rm= TRUE))
gen.annual.wide.ipp
```


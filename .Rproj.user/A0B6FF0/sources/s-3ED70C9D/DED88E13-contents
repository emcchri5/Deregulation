  theme(panel.background = element_rect(fill = "beige")) +
  theme(axis.text.x = element_text(color = "tan4", angle = 55, hjust = 1)) +
  theme(axis.text.y = element_text(color = "tan4")) +
  theme(axis.title = element_text(color = "tan4", face = "bold")) +
  theme(panel.grid = element_line(color = "navajowhite2")) +
  theme(plot.title = element_text(face = "bold.italic", hjust = 1.5))
IN2 <- price.annual.origin %>%
  filter(State == "IN") %>%
  rename(ind = "Industry Sector Category") %>%
  ggplot(mapping=aes(x=Year, y = Total, color = ind)) +
  geom_line() +
  ylab("Cents per Kilowatt Hour") +
  ggtitle("Indiana Electricity Price by Sector") +
  labs(color = "Industry Sector Category") +
  scale_x_continuous(limit=c(1970, 2018), breaks = (c(1970, 1975, 1980, 1985, 1990, 1995, 2000, 2005, 2010, 2015)), minor_breaks = (seq(1970, 2018, 1))) +
  theme(axis.text.x = element_text(face = "italic", size = 8.5, angle = 75, hjust = 1)) +
  scale_y_continuous(limit=c(0, 10), breaks = (c(0, 2, 4, 6, 8, 10)))+
  theme(plot.background = element_rect(fill = "beige")) +
  theme(legend.key = element_rect(fill = "beige")) +
  theme(legend.background = element_rect(fill = "beige")) +
  theme(legend.text = element_text(color = "tan4")) +
  theme(legend.title = element_text(color = "tan4", face = "bold")) +
  theme(axis.line = element_line(colour = "tan3")) +
  theme(panel.background = element_rect(fill = "beige")) +
  theme(axis.text.x = element_text(color = "tan4", angle = 55, hjust = 1)) +
  theme(axis.text.y = element_text(color = "tan4")) +
  theme(axis.title = element_text(color = "tan4", face = "bold")) +
  theme(panel.grid = element_line(color = "navajowhite2")) +
  theme(plot.title = element_text(face = "bold.italic", hjust = 1.5))
IN3 <- revenue.annual.ind.share %>%
  filter(state == "IN") %>%
  filter(year >= 1990) %>%
  ggplot(mapping = aes(x = year, y = percent, color = service)) +
  geom_line() +
  scale_x_continuous(limit=c(1990, 2018), breaks = (c(1990, 1995, 2000, 2005, 2010, 2015)), minor_breaks = (seq(1990, 2018, 1))) +
  scale_y_continuous(limit = c(0,100), breaks = c(25,50,75,100)) + 
  xlab("Year") +
  ylab("Percent of Total Revenue (%)") +
  labs(color = "Industry Sector Category") +
  ggtitle("Indiana Share of Market Revenue by Sector") +
  theme(plot.background = element_rect(fill = "beige")) +
  theme(legend.key = element_rect(fill = "beige")) +
  theme(legend.background = element_rect(fill = "beige")) +
  theme(legend.text = element_text(color = "tan4")) +
  theme(legend.title = element_text(color = "tan4", face = "bold")) +
  theme(axis.line = element_line(colour = "tan3")) +
  theme(panel.background = element_rect(fill = "beige")) +
  theme(axis.text.x = element_text(color = "tan4", angle = 55, hjust = 1)) +
  theme(axis.text.y = element_text(color = "tan4")) +
  theme(axis.title = element_text(color = "tan4", face = "bold")) +
  theme(panel.grid = element_line(color = "navajowhite2")) +
  theme(plot.title = element_text(face = "bold.italic", hjust = 1.5))
